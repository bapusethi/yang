<!DOCTYPE html>
<html>
  <head>
    <title>Yang</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <script src="//maps.google.com/maps/api/js?key=AIzaSyDSbM-MdpVSx6rVLJoed9bvggWNqm3grNU"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
  </head>

  <body>
    <header class="navbar navbar-expand navbar-dark flex-column flex-md-row bd-navbar justify-content-end">
        <%- if(current_contact) %>
          <%= link_to('Home', '/', { :class=>"nav-link" }) %>
          <%= link_to('Account', edit_contact_path(current_contact), { :class=>"nav-link" }) %>
          <%= link_to('Pets', pets_path,{ :class=>"nav-link" }) %>
          <%= link_to('Inbox', conversations_path, { :class=>"nav-link" }) %>
          <%= link_to('Contacts', contacts_path, { :class=>"nav-link" }) %>
          <%= link_to('Logout', destroy_contact_session_path, method: :delete, :class=>"nav-link" ) %>
        <%- else %>
          <%= link_to('Home', '/', { :class=>"nav-link" }) %>
          <%= link_to('Login', new_contact_session_path, { :class=>"nav-link" }) %>
          <%= link_to('Sign up', new_contact_registration_path, { :class=>"nav-link" }) %>
        <%- end %>
    </header>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>